<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.codeclan.com.activity_mood_tracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="example.codeclan.com.activity_mood_tracker.moodTrackerActivity"
            android:label="Activity Mood Tracker">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".moodSummaryActivity"
            android:label="@string/summary_activity_title"
            android:parentActivityName="example.codeclan.com.activity_mood_tracker.moodTrackerActivity">
        </activity>



    </application>

</manifest>
